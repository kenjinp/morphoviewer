{
  "name": "morphoviewer",
  "version": "0.1.4",
  "description": "A 3D neuron morphology viewer for the web",
  "repository": "jonathanlurie/morphoviewer",
  "moduleName": "morphoviewer",
  "entry": "src/main.js",
  "main": "dist/morphoviewer.js",
  "min": "dist/morphoviewer.min.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rollup -c",
    "dev": "serve . & rollup -w -c rollup.config.dev.js",
    "doc": "documentation build src/*.js -o ./doc/ -f html; documentation build src/*.js -f md > documentation.md",
    "lintfix": "standard --fix src/**/*.js",
    "lint": "standard src/**/*.js"
  },
  "author": "Jonathan Lurie",
  "license": "MIT",
  "devDependencies": {
    "codecutils": "^0.1.3",
    "documentation": "^6.1.0",
    "morphologycorejs": "^0.1.1",
    "pako": "^1.0.6",
    "rollup": "^0.57.0",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.1.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^6.0.0",
    "serve": "^6.3.1",
    "standard": "^11.0.1",
    "three": "^0.96.0"
  },
  "dependencies": {}
}
